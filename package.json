{
  "name": "apimocker",
  "description": "Simple HTTP server that returns mock service API responses to your front end.",
  "version": "0.4.11",
  "engines": {
    "node": ">=0.10.0"
  },
  "author": "Greg Stroup <gstroup@gmail.com>",
  "dependencies": {
    "JSONPath": ">=0.10.0",
    "body-parser": "~1.4.3",
    "commander": ">=1",
    "express": "~4.5",
    "express-http-proxy": "0.6.0",
    "express-xml-bodyparser": "^0.2.2",
    "underscore": ">=1",
    "untildify": "^2.0.0"
  },
  "devDependencies": {
    "chai": ">=1.5.0",
    "grunt": "~0.4.0",
    "grunt-contrib-jshint": "~0.7.1",
    "grunt-contrib-watch": ">=0.3.1",
    "grunt-execute": "^0.2.2",
    "grunt-mocha-cli": "1.3.0",
    "mocha": ">=1.8.2",
    "sinon": ">=1.6.0"
  },
  "main": "./lib/apimocker.js",
  "bin": {
    "apimocker": "./bin/apimocker.js"
  },
  "keywords": [
    "express",
    "mock",
    "stub",
    "REST",
    "SOAP",
    "testing",
    "functional",
    "api",
    "grunt",
    "gulp"
