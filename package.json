  "repository": {
    "type": "git",
    "url": "https://github.com/gstroup/apimocker.git"
  },
  "preferGlobal": true,
  "bugs": {
    "url": "https://github.com/gstroup/apimocker/issues"
  },
  "directories": {
    "bin": "./bin",
    "lib": "./lib",
    "test": "./test",
    "samplemocks": "./samplemocks"
  },
  "scripts": {
    "test": "mocha test/*.js",
    "start": "node bin/apimocker.js -c config.json"
  },
